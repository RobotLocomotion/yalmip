function rmpath_yalmip()
  yalmip_dir='@CMAKE_CURRENT_SOURCE_DIR@';
  rmpath(yalmip_dir);
  rmpath(fullfile(yalmip_dir,'extras'));
  rmpath(fullfile(yalmip_dir,'solvers'));
  rmpath(fullfile(yalmip_dir,'modules'));
  rmpath(fullfile(yalmip_dir,'modules','parametric'));
  rmpath(fullfile(yalmip_dir,'modules','moment'));
  rmpath(fullfile(yalmip_dir,'modules','global'));
  rmpath(fullfile(yalmip_dir,'modules','sos'));
  rmpath(fullfile(yalmip_dir,'operators'));
